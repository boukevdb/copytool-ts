{
  "name": "tov-tool",
  "version": "1.0.0",
  "description": "Een TypeScript tool",
  "main": "dist/index.js",
  "scripts": {
    "dev": "vite",
    "server": "node server.js",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "test": "jest",
    "serve": "npx http-server dist -p 3000 -o",
    "db:init": "tsx src/database/init.ts",
    "db:reset": "rm -rf data && npm run db:init"
  },
